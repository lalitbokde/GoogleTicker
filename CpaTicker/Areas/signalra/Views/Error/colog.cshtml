@model IEnumerable<CpaTicker.Areas.admin.Classes.ConversionLog> 

@{
    ViewBag.Title = "colog";
    TimeZoneInfo tzi = TimeZoneInfo.FindSystemTimeZoneById((string)ViewBag.tzid);
}

<table class="table">
    <thead>
        <tr>
           @* <th>@Html.DisplayNameFor(m => m.ConversionLogId)</th>*@
            <th>@Html.DisplayNameFor(m => m.ConversionDate)</th>
            <th>@Html.DisplayNameFor(m => m.IPAddress)</th>
            <th>@Html.DisplayNameFor(m => m.Success)</th>
            <th>@Html.DisplayNameFor(m => m.Reason)</th>
            <th>@Html.DisplayNameFor(m => m.PixelsFound)</th>
             <th>@Html.DisplayNameFor(m => m.Postback)</th>
            <th>@Html.DisplayNameFor(m => m.TransactionId)</th>
            <th>@Html.DisplayNameFor(m => m.Output)</th>
            <th>@Html.DisplayNameFor(m => m.AffiliateId)</th>
             <th>@Html.DisplayNameFor(m => m.CustomerId)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
               @* <td>
                    @Html.DisplayFor(modelItem => item.ConversionLogId)
                </td>*@
                <td>
                    @TimeZoneInfo.ConvertTimeFromUtc(item.ConversionDate, tzi)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IPAddress)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Success)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Reason)
                </td>
                 <td>
                    @Html.DisplayFor(modelItem => item.PixelsFound)
                </td>
                 <td>
                    @Html.DisplayFor(modelItem => item.Postback)
                </td>
                 <td>
                    @Html.DisplayFor(modelItem => item.TransactionId)
                </td>
                 <td>
                    @Html.DisplayFor(modelItem => item.Output)
                </td>
                 <td>
                    @Html.DisplayFor(modelItem => item.AffiliateId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CustomerId)
                </td>
            </tr>
            
        }
    </tbody>
</table>
