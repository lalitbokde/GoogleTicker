        function SelectLevelPrice() {
            $('#levelSelected').html($('#Level').val());
            var levelPrice = '0.00';
            switch ($('#Level').val()) {
                case 'Gold':
                    levelPrice = '279.00';
                    break;
                case 'Platinum':
                    levelPrice = '799.00';
                    break;
                case 'Diamond':
                    levelPrice = '999.00';
                    break;
            }
            $('#priceSelected').html(levelPrice);
        }
        $(document).ready(function () {
            SelectLevelPrice();
            $('#Level').change(function () {
                SelectLevelPrice();
            });
            $('#countrylist').change(function () {
                $.getJSON('/account/getstates/' + $('#countrylist').val(), null, function (data) {
                    if (data.length > 0) {
                        var combo = document.getElementById('statelist');

                        if (combo == null) {
                            var div = $('#stateDiv');
                            combo = '<label class="select"><select name="SelectedState">'
                            $.each(data, function (i, item) {
                                combo = combo + '<option value="' + item.StateCode + '">' + item.StateName + '</option>'
                            });
                            combo = combo + '<i></i></select></label>';
                            div.html(combo);
                        }

                        else {
                            while (combo.options.length > 0) {
                                combo.options[combo.options.length - 1] = null;
                            }

                            $.each(data, function (i, item) {
                                combo.options[i] = new Option();
                                combo.options[i].text = item.StateName;
                                combo.options[i].value = item.StateCode;
                            });
                        }
                    }
                    else {
                        var div = $('#stateDiv');
                        div.html('<label class="input"><input name="SelectedState" type="text" value="" /></label>');
                    }
